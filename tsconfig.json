{
  "compilerOptions": {
    "jsx": "react-jsx",
    "sourceMap": true,
    "outDir": "dist",
    "strict": true,
    "target": "es5",
    "module": "commonjs",
    "resolveJsonModule": true,
    "rootDir": ".",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "baseUrl": "./"
  },
  "exclude": [
    "node_modules",
  ]
}
